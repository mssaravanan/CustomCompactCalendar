<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="compactCalendarEventIndicatorStyle" format="enum">
        <enum name="fill_large_indicator" value="1" />
        <enum name="no_fill_large_indicator" value="2" />
        <enum name="small_indicator" value="3" />
    </attr>
    <attr name="compactCalendarCurrentDayIndicatorStyle" format="enum">
        <enum name="fill_large_indicator" value="1" />
        <enum name="no_fill_large_indicator" value="2" />
    </attr>
    <attr name="compactCalendarSelectedDayIndicatorStyle" format="enum">
        <enum name="fill_large_indicator" value="1" />
        <enum name="no_fill_large_indicator" value="2" />
    </attr>
    <attr name="compactCalendarWeekBackgroundStyle" format="enum">
        <enum name="fill" value="0" />
        <enum name="stroke" value="1" />
    </attr>
    <declare-styleable name="CompactCalendarView">
        <attr name="compactCalendarTextSize" format="dimension"/>
        <attr name="compactCalendarBackgroundColor" format="color"/>
        <attr name="compactCalendarCurrentDayBackgroundColor" format="color"/>
        <attr name="compactCalendarCurrentDayTextColor" format="color"/>
        <attr name="compactCalendarSelectedDayBackgroundColor" format="color"/>
        <attr name="compactCalendarSelectedDayTextColor" format="color"/>
        <attr name="compactCalendarSelectedDayIndicatorStyle"/>
        <attr name="compactCalendarTextColor" format="color"/>
        <attr name="compactCalendarMultiEventIndicatorColor" format="color"/>
        <attr name="compactCalendarTargetHeight" format="dimension"/>
        <attr name="compactCalendarEventIndicatorStyle"/>
        <attr name="compactCalendarCurrentDayIndicatorStyle"/>
        <attr name="compactCalendarDisplayOtherMonthDays" format="boolean"/>
        <attr name="compactCalendarShouldSelectFirstDayOfMonthOnScroll" format="boolean"/>
        <attr name="compactCalendarOtherMonthDaysTextColor" format="color"/>
        <attr name="compactCalendarWeekEndDaysTextColor" format="color"/>
        <attr name="compactCalendarWeekDaysTextColor" format="color"/>
        <attr name="compactCalendarRowLineColor" format="color"/>
        <attr name="compactCalendarMonthNameTextColor" format="color"/>

        <attr name="compactCalendarWeekBackgroundStyle"/>
        <attr name="compactCalendarWeekNamesBackgroundColor" format="color"/>
        <attr name="compactCalendarMinYear" format="integer"/>
        <attr name="compactCalendarMaxYear" format="integer"/>
        <attr name="compactCalendarCircleScale" format="float"></attr>
        <attr name="compactCalendarDrawRowLine" format="boolean"></attr>
        <attr name="compactCalendarApplyWeekEndColor" format="boolean"></attr>
        <attr name="compactCalendarDisplayMonthName" format="boolean"></attr>
    </declare-styleable>
</resources>
